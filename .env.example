# GitLab OAuth Configuration (Required for GitLab App mode)
GITLAB_CLIENT_ID=your_gitlab_app_client_id
GITLAB_CLIENT_SECRET=your_gitlab_app_client_secret
GITLAB_REDIRECT_URI=http://localhost:5051/oauth/callback

# GitLab Instance
GITLAB_BASE_URL=https://gitlab.com

# Legacy configuration (for backward compatibility)
GITLAB_TOKEN=your_personal_access_token

# Server Configuration
SERVER_PORT=5051
SERVER_DEBUG=true
APP_BASE_URL=http://localhost:5051

# Amp Configuration
AMP_TIMEOUT=300
AMP_SERVER_URL=https://ampcode.com
MCP_AUTH_TOKEN=your_mcp_auth_token
CRA_PUBLIC_URL=http://localhost:5051
